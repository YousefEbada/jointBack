{
  "env": { "es2022": true, "node": true },
  "parserOptions": { "sourceType": "module", "ecmaVersion": 2022 },
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "rules": {
    "import/no-unresolved": "off",
    "no-console": "warn"
  },
  "ignorePatterns": ["dist"]
}
